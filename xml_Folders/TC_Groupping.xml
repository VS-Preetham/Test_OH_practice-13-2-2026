<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite" thread-count="5">

    <groups>
        <run>
            <include name="Master"/>
            <include name="Regression"/>
            <exclude name="Sanity"/>
        </run>
    </groups>

    <test name="Test">
        <parameter name="os" value="Windows"/>
        <parameter name="browser" value="Chrome"/>

        <classes>
            <class name="testCases.TC__01_Account_Registration"/>
            <class name="testCases.TC__02_LoginPage"/>
            <class name="testCases.TC__03_DDT_LoginPage"/>
        </classes>
    </test>

</suite>
